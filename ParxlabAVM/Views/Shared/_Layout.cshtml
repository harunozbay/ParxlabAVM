        //Google Maps Api v3 Data Layer
        var map;
        function initMap() {
            map = new google.maps.Map(document.getElementById('map'), {
                center: { /*ankamall*/ lat: 39.950834, lng: 32.832705, },
                zoom: 20,
                mapTypeId: google.maps.MapTypeId.ROADMAP,
                mapTypeControl: false
            });
            map.data.setControls(['Polygon', 'LineString']);

            map.data.setStyle({
                editable: true,
                draggable: true,
                fillColor: '#90EE90',
                fillOpacity: 1,
                strokeWeight: 2,
                strokeColor: '#CD5C5C'
            });
            bindDataLayerListeners(map.data);

            //load saved data
            loadPolygons(map);

        }

        function bindDataLayerListeners(dataLayer) {
            dataLayer.addListener('addfeature', savePolygon);
            dataLayer.addListener('removefeature', savePolygon);
            dataLayer.addListener('setgeometry', savePolygon);
        }

        function loadPolygons(map) {
            var data = JSON.parse(localStorage.getItem('geoData'));

            map.data.forEach(function (f) {
                map.data.remove(f);
            });
            map.data.addGeoJson(data)
        }

        function savePolygon() {
            map.data.toGeoJson(function (json) {
                localStorage.setItem('geoData', JSON.stringify(json));
                initControls(json);
            });
        }

        //Kayıt edilen şekilleri silmek için
        function initControls(data) {
            var sel = document.getElementById('polyList');
            sel.options.length = 0;

            for (var i = 0; i < data.features.length; i++) {
                var opt = document.createElement('option');
                opt.innerHTML = data.features[i].geometry.type;
                opt.value = i;
                sel.appendChild(opt);
            }

            document.getElementById("btnDelete").disabled = (data.features.length === 0);
            document.getElementById("btnDelete").onclick = function () {
                var selIdx = sel.options[sel.selectedIndex].value; //get poly index
                data.features.splice(parseInt(selIdx), 1);

                //reload
                localStorage.setItem('geoData', JSON.stringify(data));
                initControls(data);
                loadPolygons(map);
            };
        }
        initMap();

    </script>




    <script>
        $(function () {
