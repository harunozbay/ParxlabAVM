
@model List<ParxlabAVM.Models.anatablo>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<table class="table">
    <tr>
        <th>
            cihaz
        </th>
        <th>
            park
        </th>
        <th>
            giriş zamanı
        </th>
        <th>
            çıkış zamanı
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.cihazid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.parkid)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.giriszamani)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.cikiszamani)
            </td>
        </tr>
    }

</table>