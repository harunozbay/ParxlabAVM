@using ParxlabAVM.Helpers
@model List<ZamanAraligiVerisi>


@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            Başlangıç
        </th>
        <th>
            Bitiş
        </th>
        <th>
            Değer
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Baslangic)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bitis)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Deger)
            </td>
        </tr>
    }

</table>
